{
    "name": "EPL Clean Dark",
    "dataColors": [
      "#F44336", "#2196F3", "#4CAF50", "#FF9800",
      "#9C27B0", "#00BCD4", "#FFC107", "#8BC34A",
      "#E91E63", "#3F51B5", "#009688", "#CDDC39"
    ],
    "visualStyles": {
      "*": {
        "*": {
          "title": [
            {
              "show": true,
              "fontSize": 13,
              "fontFamily": "Segoe UI Semibold",
              "color": "#FFFFFF"
            }
          ],
          "background": [
            {
              "show": true,
              "color": {
                "solid": {
                  "color": "#1E1E1E"
                }
              },
              "transparency": 0
            }
          ],
          "border": [
            {
              "show": false
            }
          ]
        }
      },
      "slicer": {
        "*": {
          "background": [
            {
              "color": {
                "solid": {
                  "color": "#252526"
                }
              },
              "transparency": 0
            }
          ],
          "header": [
            {
              "fontSize": 11,
              "fontFamily": "Segoe UI",
              "color": "#FFFFFF"
            }
          ]
        }
      }
    },
    "textClasses": {
      "callout": {
        "color": "#FFFFFF"
      }
    },
    "background": "#121212",
    "foreground": "#FFFFFF",
    "tableAccent": "#2196F3"
  }
  